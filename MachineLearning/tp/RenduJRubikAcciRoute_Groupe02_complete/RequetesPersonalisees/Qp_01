  with member [Measures].[total_blesses] as '([Measures].[SUM of MAccident.nb_blesses_graves] + [Measures].[SUM of MAccident.nb_blesses_legers])'
select Filter({[Measures].[total_blesses], [Measures].[SUM of MAccident.nb_blesses_legers], [Measures].[SUM of MAccident.nb_blesses_graves]}, ([Measures].[total_blesses] &gt; 0.0)) ON COLUMNS,
  TopCount(Order({[Lieu.Lieu].[Departement].Members}, [Measures].[total_blesses], DESC), 5.0) ON ROWS
from [cubeacciroute1]
