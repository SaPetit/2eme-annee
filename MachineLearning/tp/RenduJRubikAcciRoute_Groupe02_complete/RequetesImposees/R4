  TopCount(Order({[Lieu.Lieu].[All Lieu.Lieus].[AUVERGNE RHONE ALPES].[SAVOIE].Children}, [Measures].[total_blesses], DESC), 20.0) ON ROWS
from [cubeacciroute1]
